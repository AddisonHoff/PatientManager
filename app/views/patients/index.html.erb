<div class="container">
  <div class="page-header">
    <h1>Patients</h1>
  </div>
</div>
<div class="container">
  <table class="table table-striped">
    <thead>
      <tr>
        <th>Name</th>
        <th>Content</th>
        <th>Age</th>
        <th>Actions</th>
      </tr>
    </thead>
    <tbody>
      <% @patients.each do |patient| %>
        <tr>
          <td><%= link_to patient.name, patient%></td>
          <td><%= patient.notes %></td>
          <td><%= patient.age %></td>
          <td> --- </td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
